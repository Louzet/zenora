{
  "name": "louzet/zenora",
  "description": "A themed PHP CLI toolkit with attributes, task runner, progress bars, and widgets",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cli",
    "terminal",
    "progress",
    "spinner",
    "table",
    "command",
    "attributes"
  ],
  "authors": [
    {
      "name": "mlouzet",
      "email": "micklouzet@hotmail.fr"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  },

  "autoload": {
    "psr-4": {
      "Zenora\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zenora\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
